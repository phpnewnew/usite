	`id` BIGINT(20) NOT NULL,
	`name` VARCHAR(50) NULL DEFAULT NULL,
	`time` TIME NULL DEFAULT NULL,
	`price` DOUBLE NULL DEFAULT NULL,
	`description` VARCHAR(500) NULL DEFAULT NULL,
	UNIQUE INDEX `name` (`name`),
	INDEX `id` (`id`)
)COLLATE='gbk_chinese_ci' 
ENGINE=InnoDB 
